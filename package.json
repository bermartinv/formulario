{
  "name": "FORMMD",
  "version": "1.0.0",
  "description": "Form material design",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass form/sass/main.scss form/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass form/sass/main.scss form/css/style.comp.css",
    "concat:css": "concat -o form/css/style.concat.css form/css/icon-font.css form/css/style.comp.css",
    "prefix:css": "postcss -npm use autoprefixer -b \"last 10 versions\" form/css/style.concat.css -o form/css/style.prefix.css",
    "compress:css": "node-sass form/css/style.prefix.css form/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bermartinv/form.git"
  },
  "author": "Bernardo Mart√≠n Viudez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bermartinv/theme1/issues"
  },
  "homepage": "https://github.com/bermartinv/theme1#readme",
  "devDependencies": {
    "node-sass": "^4.14.1"
  },
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-cli": "^6.1.3"
  }
}
